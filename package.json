{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.vue",
    "format": "prettier --write \"src/**/*.{ts,tsx,vue,js,json,css,scss}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@vueuse/core": "^12.5.0",
    "axios": "^1.13.2",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.13",
    "vue-router": "^4.6.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.15.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.5.1",
    "happy-dom": "^15.11.7",
    "prettier": "^3.6.2",
    "sass": "^1.83.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^2.1.4",
    "vue-tsc": "^2.2.8",
    "@playwright/test": "^1.48.0"
  }
